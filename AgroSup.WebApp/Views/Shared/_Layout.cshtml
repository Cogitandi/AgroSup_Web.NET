<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AgroSup</title>
    <link rel="stylesheet" href="~/fa/css/all.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/md/css/mdb.min.css">
    <link rel="stylesheet" href="~/css/site.css" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <header>

        <section id="intro" class='margin-bottom-3'>
            <nav class="navbar navbar-expand-lg navbar-dark bg-transparent">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
                    <a class="navbar-brand text" asp-area="" asp-controller="Home" asp-action="Index">AgroSup</a>

                    <ul class="nav navbar-nav mr-auto"></ul>
                    <partial name="_LoginPartial" />

                </div>
            </nav>
        </section>
    </header>
    <main role="main">
        <section id="content">
            <div class="container-fluid pt-2">
                @RenderBody()
            </div>
        </section>
    </main>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - AgroSup
        </div>
    </footer>
    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/md/js/popper.min.js"></script>
    <script type="text/javascript" src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="~/md/js/mdb.js"></script>
    <script type="text/javascript" src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>